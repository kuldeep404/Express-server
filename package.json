    "@babel/preset-env": "^7.5.5",
    "@types/node": "^12.6.8",
    "husky": "^3.0.1",
    "nodemon": "^1.19.1",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
